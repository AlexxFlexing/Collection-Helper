{
  "name": "collection-helper",
  "version": "1.0.4",
  "description": "Multi-platform, beautiful, feature rich collection manager for osu!",
  "homepage": "https://github.com/nzbasic/Collection-Helper-Electron",
  "author": {
    "name": "James Coppard",
    "email": "jamescoppard024@gmail.com"
  },
  "keywords": [
    "osu",
    "osu!",
    "collection",
    "angular",
    "electron",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/nzbasic/Collection-Helper.git"
  },
  "build": {
    "directories": {
      "buildResources": "resources",
      "output": "release/"
    },
    "extraResources": [
      {
        "from": "dist",
        "to": "app",
        "filter": [
          "**/*"
        ]
      }
    ],
    "productName": "Collection Helper",
    "buildVersion": "1.0.1",
    "nsis": {
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "oneClick": false
    },
    "win": {
      "publish": [
        "github"
      ]
    },
    "files": [
      "**/*",
      "!**/*.ts",
      "!*.map",
      "!package.json",
      "!package-lock.json"
    ]
  },
  "main": "app/main.js",
  "private": true,
  "scripts": {
    "postinstall": "electron-builder install-app-deps",
    "ng": "ng",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c web -o",
    "electron:serve-tsc": "tsc -p tsconfig.serve.json",
    "electron:serve": "wait-on tcp:4200 && npm run electron:serve-tsc && npx electron . --serve",
    "electron:local": "npm run build:prod && npx electron .",
    "electron:build": "npm run build:prod && electron-builder build --publish=never",
    "electron:deploy": "npm run build:prod && electron-builder build --win --publish=always",
    "test": "ng test --watch=false",
    "test:watch": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular/animations": "12.1.1",
    "@angular/cdk": "12.1.1",
    "@angular/common": "12.0.5",
    "@angular/compiler": "12.0.5",
    "@angular/core": "12.0.5",
    "@angular/forms": "12.0.5",
    "@angular/language-service": "12.0.5",
    "@angular/material": "12.1.1",
    "@angular/platform-browser": "12.0.5",
    "@angular/platform-browser-dynamic": "12.0.5",
    "@angular/router": "12.0.5",
    "@ctrl/ngx-codemirror": "5.0.0",
    "@electron/remote": "1.0.4",
    "@fontsource/inter": "4.5.0",
    "@fontsource/open-sans": "4.5.0",
    "@materia-ui/ngx-monaco-editor": "5.1.0",
    "@supercharge/promise-pool": "1.7.0",
    "@trodi/electron-splashscreen": "1.0.0",
    "@types/graceful-fs": "4.1.5",
    "@types/sqlite3": "3.1.7",
    "autoprefixer": "10.2.6",
    "axios": "0.21.1",
    "codemirror": "5.62.0",
    "express": "4.17.1",
    "find-free-ports": "2.0.3",
    "get-port-electron": "5.0.0",
    "graceful-fs": "4.2.6",
    "monaco-editor": "0.25.2",
    "ngx-codemirror": "1.0.0",
    "ngx-dropdown-list": "1.1.2",
    "ngx-easy-table": "15.0.0",
    "ngx-monaco-editor": "12.0.0",
    "ngx-select-dropdown": "2.1.0",
    "ngx-toastr": "14.0.0",
    "postcss": "8.3.5",
    "reverse-line-reader": "0.2.6",
    "rxjs": "6.6.6",
    "sqlite": "4.0.23",
    "sqlite3": "5.0.2",
    "tailwindcss": "2.2.4",
    "tiny-async-pool": "1.2.0",
    "ts-debounce": "3.0.0",
    "tslib": "2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "12.0.0",
    "@angular-devkit/build-angular": "12.0.5",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "12.0.5",
    "@angular/compiler-cli": "12.0.5",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/jasmine": "3.7.6",
    "@types/jasminewd2": "2.0.9",
    "@types/mocha": "8.2.2",
    "@types/node": "15.6.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "cross-env": "7.0.3",
    "electron": "13.0.1",
    "electron-builder": "22.10.5",
    "electron-packager": "15.2.0",
    "electron-reload": "1.5.0",
    "eslint": "7.27.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.2",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.0.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "mocha": "8.4.0",
    "nan": "2.14.2",
    "npm-run-all": "4.1.5",
    "spectron": "15.0.0",
    "ts-node": "9.1.1",
    "typescript": "4.2.4",
    "wait-on": "5.0.1",
    "webdriver-manager": "12.1.8"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "chrome 83"
  ]
}
